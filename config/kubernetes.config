############################################################
# KUBERNETES CONFIG 
############################################################

# DOCKER & CLUSTER
#export KUBECONFIGDIR=			# directory for kubeconfig
#export KUBECONFIG=			# path to kubeconfig file
export DOCKER_REGISTRY_URL=192.168.1.143:5000		# host & port of registry
export DOCKER_EMAIL=""			# login info for registry
export DOCKER_USERNAME=""
export DOCKER_PASSWORD=""

# DAP CLUSTER ARTIFACTS
export CONJUR_CONFIG_MAP=conjur-config		# conjur configuration data
export TEST_APP_NAMESPACE_NAME=testapps		# namespace for apps
export AUTHENTICATOR_ID=dev-atx 		# unique identifier for the k8s cluster

###########################################################
# Values below should not be changed
###########################################################

export CLI=kubectl
export CONJUR_SERVICEACCOUNT_NAME=conjur-cluster
export CONJUR_FOLLOWER_COUNT=1
export AUTHENTICATOR_CLIENT_IMAGE=cyberark/conjur-authn-k8s-client:latest
export SEED_FETCHER_IMAGE=seed-fetcher:latest
export IMAGE_PULL_POLICY=IfNotPresent
export APP_CONFIG_MAP=$TEST_APP_NAMESPACE_NAME-config	# app configuration data
export CONJUR_AUTHENTICATORS=authn,authn-k8s/$AUTHENTICATOR_ID
export CONJUR_CLUSTER_LOGIN="host/conjur/authn-k8s/$AUTHENTICATOR_ID/apps/$CONJUR_NAMESPACE_NAME/service_account/$CONJUR_SERVICEACCOUNT_NAME"
# Dummy users for OpenShift RBAC
export CLUSTER_ADMIN_USERNAME=foo	# Cluster admin role
export FOLLOWER_ADMIN_USERNAME=bar	# DAP Follower admin role
export DEVELOPER_USERNAME=baz		# Developer role
